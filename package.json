{
  "name": "zoo-app",
  "version": "0.0.1",
  "description": "zoo app",
  "repository": "git@github.com:zoo-app-bel/zoo.git",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "husky install",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "npm run eslint && npm run prettier && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix && npm run stylelint:fix",
    "eslint": "npx eslint --ext ts,tsx ./ ",
    "eslint:fix": "npx eslint --ext ts,tsx ./ --fix",
    "prettier": "npx prettier --check ./",
    "prettier:fix": "npx prettier --write ./",
    "stylelint": "npx stylelint **/*.scss **/*.css",
    "stylelint:fix": "npx stylelint **/*.scss **/*.css --fix"
  },
  "keywords": [
    "next.js"
  ],
  "author": "I am",
  "license": "ISC",
  "dependencies": {
    "clsx": "^1.1.1",
    "next": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "sass": "^1.32.7",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.4"
  }
}
